@model EntityLayer_HizmetPortal.Concrate.Stage

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>güncelleme sayfası</h1>
<br />
@using (Html.BeginForm("Edit", "AdminStage", FormMethod.Post))
{
    <div class="form-group">
        @Html.Label("Stage ID")
        @Html.TextBoxFor(x => x.StageId, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.Label("Stage Name")
        @Html.TextBoxFor(x => x.StageName, new { @class = "form-control" })
    </div>
    <button type="submit" class="btn btn-success">Güncelle</button>
}